package course;

import java.util.Locale;
import java.util.Scanner;

public class Entradadedados2 {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		
		Scanner sc = new Scanner(System.in);
		
		int n1 = sc.nextInt();   // Ler numero
		sc.nextLine();
		String name = sc.nextLine();    //ler nome 
		char gender = sc.next().charAt(0);       //ler apenas um character na primeira posição
		String s = sc.next();
		char letter = s.charAt(0);                 // ler apenas a primeiora letra da string
		int digit = Integer.parseInt (s.substring(1));					// ler o restante da string a partir da posição 1
	    double n2 = sc.nextDouble();
		
		
		System.out.println(n1);
		System.out.println(name);
		System.out.println(gender);
		System.out.println(letter);
		System.out.println(digit);
		System.out.println(n2);
		
		
		sc.close();
		
		
	}

}
